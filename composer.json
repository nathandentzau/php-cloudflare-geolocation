{
    "name": "nathandentzau/cloudflare-geolocation",
    "description": "A library that assists in retrieving the continent and country information from Cloudflare's headers.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">= 7.2",
        "symfony/http-foundation": "^3.4 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "NathanDentzau\\CloudflareGeolocation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NathanDentzau\\Tests\\CloudflareGeolocation\\": "tests/src/"
        }
    }
}
